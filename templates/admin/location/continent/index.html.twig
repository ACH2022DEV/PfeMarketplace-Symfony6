<div class="card container shadow mt-5"  >
    <div>
        <button
                class="btn btn-sm btn-success"
                style="position: relative; top: 65px; left: 20px"
                data-action="modal-form#openModal"
                data-form-url-value="{{ path('app_admin_location_continent_new' ) }}"
                data-url-value="{{ path('app_admin_location_continent_index', { ajax: 1 } ) }}"
                data-node-dest-id-value="myOneCustomContent"
                data-modal-title-value="{{ 'Buttons.Custom.Nouveau.Continent'|trans }}"
                data-refresh-key-value="refresh-html"
        >
            <i class="fas fa-plus-circle m-1"></i> {{ 'Buttons.General.Nouveau'|trans }}
        </button>

    </div>
    <div class="card-body table-responsive p-3 text-center">
        <table class="table"  data-search= "true"
               data-toggle="table"  data-pagination="true" data-page-size="10"
               data-show-pagination-switch="true" data-show-columns="true">
            <thead>
            <tr>
                <th>Code</th>
                <th>Name</th>
                <th>Active</th>
                <th>actions</th>
            </tr>
            </thead>
            <tbody data-reload-content-target="myOneCustomContent">
            {{ include('admin/location/continent/_list.html.twig') }}
            </tbody>
        </table>
    </div>
</div>



