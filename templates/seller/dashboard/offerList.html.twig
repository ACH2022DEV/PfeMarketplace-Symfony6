                <thead class="bg-light shadow-sm ">
                <tr class="m-3 table-light rounded-top-2 ">
                    <th class="fw-bold mb-0 text-dark fs-5">Offer Name</th>
                    {% for product in productType %}
                        <th class="fw-bold mb-0 text-dark fs-5">{{ product.name }}</th>
                    {% endfor %}
                    <th class="fw-bold mb-0 text-dark fs-5 justify-content-center">Price</th>
                    <th class="fw-bold mb-0 text-dark fs-5 justify-content-center">MaxItems</th>
                    <th class="fw-bold mb-0 text-dark fs-5 justify-content-center">number of days</th>
                    <th class="fw-bold mb-0 text-dark fs-5 text-start">Start Date</th>
                    <th class="fw-bold mb-0 text-dark fs-5 justify-content-center">Action</th>


                </tr>
                </thead>
                <tbody>
                {% for of in offer %}
                    {% set total = 0 %}
                    {#                    {% set totalMaxitem = 0 %}#}

                    {% for offerProductType in of.offerProductTypes %}
                        {% set total = total + offerProductType.price %}
                        {#                        {% set totalMaxitem = totalMaxitem + offerProductType.maxItems %}#}
                    {% endfor %}

                    <tr>

                        <td>
                            <div class="d-flex align-items-center">
                                <div class="ms-3">
                                    <p class="fw-bold mb-1 fs-5">{{ of.name }}</p>
                                </div>
                            </div>
                        </td>
                        {% for product in productType %}
                            <td>
                                {% set offerProductTypes = [] %}

                                {% for offerProductType in of.offerProductTypes %}
                                    {% set offerProductTypes = offerProductTypes|merge([offerProductType.productType.name]) %}
                                {% endfor %}

                                {% if product.name in offerProductTypes %}
                                    <svg xmlns="http://www.w3.org/2000/svg"  width="32" style="color: green" height="32" fill="currentColor" class="bi bi-check2-circle " viewBox="0 0 16 16">
                                        <path d="M2.5 8a5.5 5.5 0 0 1 8.25-4.764.5.5 0 0 0 .5-.866A6.5 6.5 0 1 0 14.5 8a.5.5 0 0 0-1 0 5.5 5.5 0 1 1-11 0z"/>
                                        <path d="M15.354 3.354a.5.5 0 0 0-.708-.708L8 9.293 5.354 6.646a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0l7-7z"/>
                                    </svg>
                                {% else %}
                                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" style="color: red" fill="currentColor" class="bi bi-x-circle" viewBox="0 0 16 16">
                                        <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                                        <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                                    </svg>
                                {% endif %}
                            </td>
                        {% endfor %}
                        <td>
                            <p class="fw-bold mb-0 text-dark justify-content-center" style=" font-size: 16px;">{{ total }} Dt</p>
                        </td>

                        <td>
                            {% set maxItems = [] %}
                            {% for offerProductType in of.offerProductTypes %}
                                {% if offerProductType.maxItems is defined %}
                                    {% set maxItems = maxItems|merge([offerProductType.maxItems]) %}
                                {% endif %}
                            {% endfor %}
                            <p class="fw-bold mb-0 text-dark justify-content-center" style="font-size: 16px;">
                                {{ maxItems|join(' / ') }} Max
                            </p>
                        </td>



                        <td>
                            <p class="fw-bold mb-0 text-dark justify-content-center" style=" font-size: 16px;">{{ of.nbDays }} Days</p>
                        </td>
                        <td>
                            <div class=" mt-2">
                                <input type="date" class="text-center p-2  w-75  rounded-pill shadow-sm">
                            </div>
                        </td>
                        <td>
                            <form action="{{ path('Add_Seller_Offer', {'id': of.id}) }}" method="POST">
                                <button class=" purple-bg rounded-circle shadow p-2 border border-0"  type="submit"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-cart-check-fill text-white" viewBox="0 0 16 16">
                                        <path d="M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1H.5zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm-1.646-7.646-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 1 1 .708-.708L8 8.293l2.646-2.647a.5.5 0 0 1 .708.708z"/>
                                    </svg></button>
                            </form>
                        </td>


                    </tr>

                {% endfor %}
                </tbody>
            </table>






